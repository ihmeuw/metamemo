{
  "extends": "tslint:recommended",
  "rules": {
    "member-access": [true, "no-public"],
    "trailing-comma": [
      true,
      {
        "multiline": {
          "arrays": "always",
          "functions": "never",
          "objects": "always"
        }
      }
    ],
    "quotemark": [true, "single", "avoid-escape"]
  }
}